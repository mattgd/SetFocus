{
  "manifest_version": 2,

  "name": "SetFocus",
  "description": "Set the default focused element when the page loads.",
  "version": "1.0",
  "permissions": ["tabs", "<all_urls>"],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["jquery-3.1.0.min.js", "content_script.js"],
    "css":["style.css"]
  }]
}
